{
  "schema": 1,
  "description": "Turns your intent into a SAFE DOS batch script",
  "type": "completion",
  "models": [
    {
      "max_tokens": 1000,
      "temperature": 0.0,
      "top_p": 0.0,
      "presence_penalty": 0.0,
      "frequency_penalty": 0.0,
      "stop_sequences": [
        "exit /b %ERRORLEVEL%",
        "exit /b 1",
        "exit /b 0"
      ]
    }
  ],
"input": {
    "parameters": [
      {
        "name": "input",
        "description": "The code gen instructions",
        "defaultValue": ""
      },
      {
        "name": "firstName",
        "description": "The user's first name",
        "defaultValue": "PAL"
      }
    ]
  }
}