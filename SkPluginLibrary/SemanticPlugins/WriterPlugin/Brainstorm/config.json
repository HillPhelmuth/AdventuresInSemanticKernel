{
  "schema": 1,
  "type": "completion",
  "description": "Given a goal or topic description generate a list of ideas",
  "models": [
    {
      "max_tokens": 2000,
      "temperature": 0.5,
      "top_p": 1.0,
      "presence_penalty": 0.0,
      "frequency_penalty": 0.0,
      "stop_sequences": [ "##END##" ]
    }
  ],
  "input": {
    "parameters": [
      {
        "name": "topic",
        "description": "A topic description or goal.",
        "defaultValue": ""
      }
    ]
  }
}
