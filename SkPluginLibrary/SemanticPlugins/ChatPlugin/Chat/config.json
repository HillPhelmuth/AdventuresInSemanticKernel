{
  "schema": 1,
  "description": "Chat with the AI",
  "type": "completion",
  "completion": {
    "max_tokens": 450,
    "temperature": 0.9,
    "top_p": 1.0,
    "presence_penalty": 0.6,
    "frequency_penalty": 0.0,
    "stop_sequences": [
      "Human:",
      "AI:"
    ]
  },
  "input": {
    "parameters": [
      {
        "name": "input",
        "description": "current input",
        "defaultValue": ""
      },
      {
        "name": "history",
        "description": "The total chat history",
        "defaultValue": ""
      },
      {
        "name": "user",
        "description": "user's name",
        "defaultValue": ""
      },
      {
        "name": "bot",
        "description": "bot name",
        "defaultValue": ""
      }
    ]
  }
}